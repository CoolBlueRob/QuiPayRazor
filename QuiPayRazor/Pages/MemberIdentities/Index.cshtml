@page
@model QuiPayRazor.Pages.MemberIdentities.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MemberIdentity[0].MemberDetailsState)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MemberIdentity[0].Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MemberIdentity[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MemberIdentity[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MemberIdentity[0].WhenCreated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MemberIdentity[0].Member)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.MemberIdentity) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MemberDetailsState)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WhenCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Member.ID)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
